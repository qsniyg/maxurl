msgstr "Под курсором"

#. mouseover_position.options.beside_cursor.name
msgid "Beside cursor"
msgstr "Рядом с курсором"

#. mouseover_position.options.center.name
msgid "Page middle"
msgstr "Середина страницы"

#. mouseover_prevent_cursor_overlap.name
msgid "Prevent cursor overlap"
msgstr "Предотвратить перекрытие курсора"

#. mouseover_prevent_cursor_overlap.description
msgid "Prevents the image from overlapping with the cursor"
msgstr "Предотвращает перекрытие изображение с курсором."

#. mouseover_overflow_position_center.name
msgid "Center popup on overflow"
msgstr ""

#. mouseover_overflow_position_center.description
msgid "Centers the popup if its initial size overflows"
msgstr ""

#. mouseover_overflow_origin.name
msgid "Overflow origin"
msgstr ""

