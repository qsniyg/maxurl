#. mouseover_move_with_cursor.name
msgid "Move with cursor"
msgstr "Перемещение с помощью курсора"

#. mouseover_move_with_cursor.description
msgid "Moves the popup as the cursor moves"
msgstr "Перемещает всплывающее окно по мере перемещения курсора."

#. mouseover_move_within_page.name
msgid "Move within page"
msgstr "Перемещение внутри страницы"

#. mouseover_move_within_page.description
msgid "Ensures the popup doesn't leave the page"
msgstr "Гарантирует, что всплывающее окно не покинет страницу."

#. zoom_out_to_close.name
msgid "Zoom out fully to close"
msgstr "Закрывать при полном уменьшении масштаба"

#. zoom_out_to_close.description
msgid "Closes the popup if you zoom out past the minimum zoom"
msgstr "Закрывает всплывающее окно, если вы уменьшаете масштаб, превышающий минимальный."

#. scroll_past_gallery_end_to_close.name
msgid "Scroll past gallery end to close"
msgstr "Закрывать при прокручивании до конца галереи"

#. scroll_past_gallery_end_to_close.description
msgid "Closes the popup if you scroll past the end of the gallery"
msgstr "Закрывает всплывающее окно, если вы прокручиваете его до конца галереи."

#. mouseover_position.name
msgid "Popup position"
msgstr "Положение всплывающего окна"

#. mouseover_position.description
msgid "Where the popup will appear"
msgstr "Где появится всплывающее окно."

#. mouseover_position.options.cursor.name
msgid "Cursor middle"
msgstr "Середина курсора"

#. mouseover_position.options.cursor.description
